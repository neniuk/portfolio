{
    "name": "client",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build --emptyOutDir",
        "build:to:server": "tsc && vite build --outDir=../server/dist/client --emptyOutDir",
        "build:analyze": "vite-bundle-visualizer --output reports/stats.html",
        "preview": "vite preview",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-draggable": "^4.4.6",
        "socket.io-client": "^4.7.2",
        "validator": "^13.12.0"
    },
    "devDependencies": {
        "@types/react": "^18.2.43",
        "@types/react-dom": "^18.2.17",
        "@types/validator": "^13.11.10",
        "@vitejs/plugin-react": "^4.3.4",
        "autoprefixer": "^10.4.16",
        "postcss": "^8.4.32",
        "prettier": "^3.1.1",
        "prettier-plugin-tailwindcss": "^0.5.11",
        "tailwindcss": "^3.4.0",
        "typescript": "^5.3.3",
        "vite": "^6.2.5",
        "vite-bundle-visualizer": "^1.2.1"
    }
}
